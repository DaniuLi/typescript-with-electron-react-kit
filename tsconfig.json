{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": false,
    "noUnusedLocals": true,
    "inlineSourceMap": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitAny": true
  },
  "include": ["src", "typings", "fuse.ts"]
}
